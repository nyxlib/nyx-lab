{
    "name": "nyx-dashboard",
    "description": "Nyx Dashboard",
    "version": "1.0.0",
    "type": "module",
    "private": true,
    "scripts": {
        "app-dev": "npm i --update vue-nyx && cd ./app/ && ../node_modules/vite/bin/vite.js --port 1234 dev",
        "app-prod": "npm i --update vue-nyx && cd ./app/ && ../node_modules/vite/bin/vite.js build",
        "tauri-dev": "cd ./tauri/ && ../node_modules/@tauri-apps/cli/tauri.js dev",
        "tauri-prod": "cd ./tauri/ && ../node_modules/@tauri-apps/cli/tauri.js build",
        "lint": "cd ./app/ && ../node_modules/eslint/bin/eslint.js"
    },
    "dependencies": {
        "@tauri-apps/api": "^2.0.0-rc.0",
        "@tauri-apps/plugin-dialog": "^2.0.0-rc.0",
        "@tauri-apps/plugin-fs": "^2.0.0-rc.0",
        "@tauri-apps/plugin-notification": "^2.0.0-rc.0",
        "@tauri-apps/plugin-shell": "^2.0.0-rc.0",
        "@vueform/multiselect": "^2.6.9",
        "bootstrap": "^5.3.3",
        "bootstrap-icons": "^1.11.3",
        "chart.js": "^4.4.3",
        "pinia": "^2.2.1",
        "split.js": "^1.6.5",
        "uuid": "^10.0.0",
        "vue": "^3.4.37",
        "vue-nyx": "file:../nyx-vue",
        "vue-router": "^4.4.3"
    },
    "devDependencies": {
        "@tauri-apps/cli": "^2.0.0-rc.3",
        "@vitejs/plugin-vue": "^5.1.2",
        "@vue/eslint-config-prettier": "^9.0.0",
        "eslint": "^9.9.0",
        "eslint-plugin-vue": "^9.27.0",
        "sass": "^1.77.8",
        "vite": "^5.4.0",
        "vite-plugin-eslint": "^1.8.1"
    },
    "author": "Jérôme Odier",
    "license": "CeCILL",
    "repository": {
        "type": "git",
        "url": "https://gitlab.in2p3.fr/lpsc-kid/nyx-dashboard"
    }
}

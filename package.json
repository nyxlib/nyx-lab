        "chartjs-adapter-date-fns": "^3.0.0",
        "d3": "^7.9.0",
        "date-fns": "^3.6.0",
        "leaflet": "^1.9.4",
        "pinia": "^2.1.7",
        "split.js": "^1.6.5",
        "uuid": "^9.0.1",
        "vue": "^3.4.21",
        "vue-indi": "file:../indi-vue",
        "vue-router": "^4.3.0"
    },
